    "@types/nodemailer": "^6.4.17",
    "animate.css": "^4.1.1",
    "axios": "^1.7.9",
    "firebase": "^11.3.1",
    "framer-motion": "^11.12.0",
    "lucide-react": "^0.344.0",
    "nodemailer": "^6.10.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.2.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.1.3",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.13.1",
    "react-loader-spinner": "^6.1.6",
    "react-player": "^2.16.0",
